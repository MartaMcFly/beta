<p id="notice"><%= notice %></p>

<h1>Missions</h1>

<table>
  <thead>
    <tr>
      <th>Nom</th>
      <th>Date debut</th>
      <th>Date fin</th>
      <th>Lieu</th>
      <th>Descriptif</th>
      <th>Objectifs</th>
      <th>Competences</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @missions.each do |mission| %>
      <tr>
        <td><%= mission.nom %></td>
        <td><%= mission.date_debut %></td>
        <td><%= mission.date_fin %></td>
        <td><%= mission.lieu %></td>
        <td><%= mission.descriptif %></td>
        <td><%= mission.objectifs %></td>
        <td><%= mission.competences %></td>
        <td><%= link_to 'Show', mission %></td>
        <td><%= link_to 'Edit', edit_mission_path(mission) %></td>
        <td><%= link_to 'Destroy', mission, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Mission', new_mission_path %>
